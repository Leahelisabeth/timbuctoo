[java]
  source_level = 8
  target_level = 8

[maven_repositories]
  huygens = http://maven.huygens.knaw.nl/repository/
  mbknor = https://raw.githubusercontent.com/mbknor/mbknor.github.com/master/m2repo/releases
  central = https://repo1.maven.org/maven2

[project]
  allow_symlinks = forbid
  build_file_search_method = watchman
  ignore = .git, .idea
  enable_build_file_sandboxing = true
